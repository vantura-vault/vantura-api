{
  "name": "vantura-api",
  "version": "1.0.0",
  "type": "module",
  "description": "Vantura backend API server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && mkdir -p dist/config && cp -r src/config/platform-rules dist/config/",
    "start": "npx prisma migrate deploy && node dist/index.js",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "npx prisma db seed",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "postinstall": "npx prisma generate"
  },
  "keywords": [
    "vantura",
    "api",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@prisma/client": "^6.17.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/node": "^24.9.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "ioredis": "^5.8.2",
    "openai": "^6.8.1",
    "prisma": "^6.17.0",
    "socket.io": "^4.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/ioredis": "^4.28.10",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.37.0",
    "ts-node-dev": "^2.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
